<div class="row pos-header" id="pos-head" style="background-color:#9b58b5">
    <div class="col-sm-12">
        <table class="table pos-table-title">
            <tbody>
                <tr>
                    <td style="width:50%"><small>V. {{version}}</small>
                        <big>&nbsp;{{company_name}}</big>
                    </td>
                    <td>
                        <div class="" style="font-size:small">
                            <ul class="nav nav-pills nav-user-menu">
                                {{#if cashier_name}}
                                    <li role="presentation" class="active">
                                        <a><strong>Cashier : </strong>{{cashier_name}}</a>
                                    </li>
                                {{/if}}
                                <li role="presentation" class="active">
                                    <a >{{user_name}}</a>
                                </li>
                                <li class="btn-settings" role="presentation"><a href="#">Settings</a></li>
                                <li class="btn-logout" role="presentation"><a href="#">Logout</a></li>
                            </ul>
                        </div>
                    </td>
                </tr>
            </tbody>
        </table>
    </div>
    <div class="col-sm-12">
      <div class="container" style="width:100%;padding:0px;">
          <nav class="navbar navbar-default nav-pos" role="navigation">
              <div class="container">
                <div class="navbar-header">
                  <a class="navbar-brand" href="#">Netforce POS</a>
                  <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                  </button>
                </div>
                <div class="navbar-header pull-right">
                </div>
                <div class="navbar-collapse collapse">
                  <ul class="nav navbar-nav">
                    {{#each menu}}
                        {{#if active}}
                            <li class="active"><a href="#{{key}}">{{value}}</a></li>
                        {{else}}
                            <li><a href="#{{key}}">{{value}}</a></li>
                        {{/if}}
                        <!--<li><a href="#">Retreive Sale</a></li>-->
                        <!--<li><a href="#">Complete Sale</a></li>-->
                    {{/each}}
                  </ul>                    
                </div>
              </div>
            </nav> 
        </div><!-- container -->
    </div>
    <div class="col-sm-12 content">
    </div>
</div>
